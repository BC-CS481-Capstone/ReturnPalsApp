<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#E1F6FF"
    tools:context=".ui.contact.SlideshowFragment">


    <TextView
        android:id="@+id/textView"
        android:layout_width="213dp"
        android:layout_height="63dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="68dp"
        android:text="Contact Us"
        android:textColor="@color/black"
        android:textSize="40sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextFirst"
        android:layout_width="173dp"
        android:layout_height="48dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="200dp"
        android:ems="10"
        android:inputType="text"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextLast"
        android:layout_width="173dp"
        android:layout_height="48dp"
        android:layout_marginStart="196dp"
        android:layout_marginTop="200dp"
        android:ems="10"
        android:inputType="text"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextTextEmailAddress"
        android:layout_width="351dp"
        android:layout_height="47dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="292dp"
        android:ems="10"
        android:inputType="textEmailAddress"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="225dp"
        android:layout_height="239dp"
        android:layout_marginStart="93dp"
        android:layout_marginBottom="98dp"
        android:src="@drawable/contact_us_screenshot_2023_09_05_at_1_53_2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="162dp"
        android:layout_height="46dp"
        android:layout_marginStart="124dp"
        android:layout_marginBottom="356dp"
        android:src="@drawable/main_login_rectangle_80"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="351dp"
        android:layout_height="47dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="292dp"
        android:src="@drawable/contact_us_rectangle_58"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textName"
        android:layout_width="63dp"
        android:layout_height="30dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="168dp"
        android:text="Name"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textEamil"
        android:layout_width="63dp"
        android:layout_height="30dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="260dp"
        android:text="Email"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textSubmit"
        android:layout_width="63dp"
        android:layout_height="30dp"
        android:layout_marginStart="48dp"
        android:layout_marginBottom="9dp"
        android:text="Submit"
        android:textColor="@color/white"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton3"
        app:layout_constraintStart_toStartOf="@+id/imageButton3" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="169dp"
        android:layout_height="42dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="204dp"
        android:src="@drawable/contact_us_rectangle_58"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="173dp"
        android:layout_height="45dp"
        android:layout_marginStart="198dp"
        android:layout_marginTop="202dp"
        android:src="@drawable/contact_us_rectangle_60"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>